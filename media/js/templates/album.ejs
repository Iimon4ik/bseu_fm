<% for (var i in albums) { %>
    <li class="span3">
        <div class="thumbnail">
            <img src="<%= $(albums[i].description).find('img').attr('src') %>" alt="<%= albums[i].title %>" style="width: 210px; height: 200px;">
            <div class="caption">
                <h4><%= albums[i].title %></h4>
                <p>Размещен: <%= (new Date(albums[i].pubDate)).getFullYear() %>-<%= (new Date(albums[i].pubDate)).getMonth() %>-<%= (new Date(albums[i].pubDate)).getDate() %></p>
                <p><a class="btn btn-danger" href="<%= albums[i].link %>">Смотреть фото</a></p>
            </div>
        </div>
    </li>
<% } %>